{
  "name": "molecule-app",
  "version": "1.0.16",
  "private": true,
  "displayName": "Molecule",
  "description": "Built with Molecule.dev",
  "author": "molecule.dev",
  "dependencies": {
    "@primer/octicons-react": "^16.1.1",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/quill": "^2.0.9",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "axios": "^0.24.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "loglevel": "^1.7.1",
    "polished": "^4.1.3",
    "quill": "^1.3.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "^4.0.3",
    "react-snap": "^1.23.0",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.2",
    "web-vitals": "^2.1.2",
    "workbox-background-sync": "^6.3.0",
    "workbox-broadcast-update": "^6.3.0",
    "workbox-cacheable-response": "^6.3.0",
    "workbox-core": "^6.3.0",
    "workbox-expiration": "^6.3.0",
    "workbox-google-analytics": "^6.3.0",
    "workbox-navigation-preload": "^6.3.0",
    "workbox-precaching": "^6.3.0",
    "workbox-range-requests": "^6.3.0",
    "workbox-routing": "^6.3.0",
    "workbox-strategies": "^6.3.0",
    "workbox-streams": "^6.3.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.20.0",
    "http-server": "^14.0.0",
    "icon-gen": "^2.1.0",
    "if-env": "^1.0.4",
    "open-cli": "^7.0.1",
    "sharp": "^0.29.2",
    "typedoc": "^0.22.8"
  },
  "scripts": {
    "postversion": "node ./scripts/writeVersion.js",
    "postinstall": "(if-env CI=true && true) || (if-env NODE_ENV=production && true) || (npm run write-dotenv-files && npm run build && npm run docs)",
    "write-dotenv-files": "node ./scripts/writeDotenvFiles.js",
    "generate-images": "node ./scripts/generateImages.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-autofix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "react-scripts test",
    "docs": "typedoc && open-cli ./docs/index.html",
    "start": "react-scripts start",
    "server": "npm run build && http-server ./build --port=3333 --proxy=http://localhost:4000",
    "snapshot-html": "react-snap && node ./scripts/updateContentSecurityPolicy.js",
    "build": "env-cmd -e build --silent react-scripts build && node ./scripts/writeVersion.js && npm run snapshot-html",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:4000",
  "reactSnap": {
    "inlineCss": true
  }
}
